	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/opi/images/serverroom.png 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/opi/images/tape_library.png 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/opi/images/xserver.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/opi/images/ 


install:  install_target install_forms install_images  FORCE

uninstall: uninstall_target uninstall_forms uninstall_images   FORCE

FORCE:

