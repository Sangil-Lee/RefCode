install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/opi/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/opi/bin/ 
	-$(INSTALL_PROGRAM) "$(TARGET)" "$(INSTALL_ROOT)/usr/local/opi/bin/$(TARGET)"
	-strip "$(INSTALL_ROOT)/usr/local/opi/bin/$(TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/local/opi/bin/$(TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/opi/bin/ 


