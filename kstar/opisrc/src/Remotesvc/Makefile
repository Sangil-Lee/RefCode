####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(OBJECTS)  
	$(LINK) -o $(TARGET) $(OBJECTS) $(OBJCOMP) $(LIBS)

dist: 
	@$(CHK_DIR_EXISTS) .obj/remotesvr1.0.0 || $(MKDIR) .obj/remotesvr1.0.0 
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .obj/remotesvr1.0.0/ && $(COPY_FILE) --parents remoteserver.cpp .obj/remotesvr1.0.0/ && (cd `dirname .obj/remotesvr1.0.0` && $(TAR) remotesvr1.0.0.tar remotesvr1.0.0 && $(COMPRESS) remotesvr1.0.0.tar) && $(MOVE) `dirname .obj/remotesvr1.0.0`/remotesvr1.0.0.tar.gz . && $(DEL_FILE) -r .obj/remotesvr1.0.0

