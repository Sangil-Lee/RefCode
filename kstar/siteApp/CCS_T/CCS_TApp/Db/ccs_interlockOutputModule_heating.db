record(bo, CCS_INTL_ECH_STOP) {
  field(DESC, "ECH Stop")
  field(SCAN, "Passive")
  field(PINI, "YES")
  field(DTYP, "Vmic2534")
  field(OMSL, "supervisory")
  field(OUT, "#C1 S16 @")
  field(ZNAM, "Low(0)")
  field(ONAM, "High(1)")
  field(ZSV, "NO_ALARM")
  field(OSV, "NO_ALARM")
  field(VAL, "1")
}

