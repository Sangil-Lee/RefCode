record (ao, "$(SYS)_KSTAR_OPR_PULSEON") {
  field(DESC, "Heating Setting for Pulse On")
  field(PREC, "2")
}

record (ao, "$(SYS)_HV_PULSE_START") {
  field(DESC, "ECH HV Set for Pulse")
  field(SCAN, "Passive")
  field(PREC, "4")
}
record (ao, "$(SYS)_HV_PULSE_LENGTH") {
  field(DESC, "ECH HV Set for Pulse")
  field(SCAN, "Passive")
  field(PREC, "4")
}
record (ao, "$(SYS)_WAVEFORM_NI_T0") {
  field(DESC, "ECH WaveForm NI T0")
  field(SCAN, "Passive")
  field(PREC, "4")
}
record (ao, "$(SYS)_WAVEFORM_NI_T1") {
  field(DESC, "ECH WaveForm NI T1")
  field(SCAN, "Passive")
  field(PREC, "4")
}


