}

record(calcout, "FUEL_PZ_PS_EMO_STOP_C") {
  field(CALC, "(A=0)?0:1")
  field(OUT, "FUEL_PZ_PS_EMO_STOP_OUT PP")
  field(INPA, "FUEL_PZ_PS_EMO_STOP")
}
