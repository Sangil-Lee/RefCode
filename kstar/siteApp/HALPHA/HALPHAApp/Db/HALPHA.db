  field(SCAN, "Passive")
  field(CALC, "A*1000")
  field(INPA, "HALPHA_sampleRate CPP NMS")
}

record(bi, HALPHA_HeartBeat) {
  field(DESC, "Analog input")
  field(INP, "HALPHA_calcHeartBeat NPP NMS")
}
record(calc, HALPHA_calcHeartBeat) {
  field(SCAN, "1 second")
  field(FLNK, "HALPHA_HeartBeat NPP NMS")
  field(CALC, "A=0?1:0")
  field(INPA, "HALPHA_HeartBeat")
}
