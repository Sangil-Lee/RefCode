record(calcout, "$(PV_NAME)_HIHI") {
  field(CALC, "A?C:B")
  field(INPA, "HDS_HCS_R_MODE CPP")
  field(INPB, "$(HIHI_CD) CPP")
  field(INPC, "$(HIHI_SB) CPP")
  field(OUT, "$(PV_NAME).HIHI CA")
}


