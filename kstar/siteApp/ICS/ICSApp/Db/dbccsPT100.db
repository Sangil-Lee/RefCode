	field(DTYP, "cFP20x0 NI compactFieldPoint")
	field(INP, "@$(SYSMON) read $(CH)")
	field(SCAN, "I/O Intr")
	field(FLNK, "$(REC)")
	field(PREC, "2")
	field(EGU, "Celsius")
}
