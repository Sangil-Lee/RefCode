	field(PREC, "2")
	field(HIHI, "35")
	field(HIGH, "30")
	field(HHSV, "MAJOR")
	field(HSV, "MINOR")
	field(LLSV, "NO_ALARM")
	field(LSV, "NO_ALARM")
}

