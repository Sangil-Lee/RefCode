record(ao, "$(SYS)_FPA_INT_TIME$(PRESET)") {
  field(DESC, "FPA integration time for preset $(PRESET)")
  field(DTYP, "IRTV AoControl")
  field(OUT, "@ao_preset_int_time $(PRESET)")
}

