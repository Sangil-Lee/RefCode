record(ai, "$(PV_NAME)_SAMPLE_RATE_READ")
{
	field(DESC, "$(PV_NAME)_SAMPLE_RATE")
	field(DTYP, "VXI1564a Read")
	field(INP, "@$(TASK) rateRead")
	field(SCAN, "I/O Intr")
	field(PREC, "7")
}
