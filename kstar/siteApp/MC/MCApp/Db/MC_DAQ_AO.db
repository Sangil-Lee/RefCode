record(ao, "$(PV_NAME)_SAMPLE_PERIOD")
{
	field(DESC,"$(PV_NAME)_SAMPLE_PERIOD")
	field(DTYP,"VXI1564a Control")
	field(OUT,"@$(TASK) smpl_period")
