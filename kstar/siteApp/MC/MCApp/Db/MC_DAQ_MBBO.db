record(mbbo, "$(PV_NAME)_RANGE_CH1")
{
        field(DESC, "$(PVDESC)_RANGE_CH1")
	field(DTYP, "VXI1564a MBBO_Control")
        field(SCAN, "Passive")
        field(PINI, "YES")
        field(OUT, "@$(TASK) $(CHAN)/ai0 ch_range")
        field(ZRVL, "0")
        field(ONVL, "1")
        field(TWVL, "2")
        field(THVL, "3")
        field(FRVL, "4")
        field(FVVL, "5")
        field(SXVL, "6")
        field(SVVL, "7")
        field(ZRST, "62.5mV Range")
        field(ONST, "0.25V Range")
        field(TWST, "1.0V Range")
        field(THST, "4.0V Range")
        field(FRST, "16V Range")
        field(FVST, "64V Range")
        field(SXST, "256V Range")
        field(SVST, "256V Range")
        field(RVAL, "7")
}
