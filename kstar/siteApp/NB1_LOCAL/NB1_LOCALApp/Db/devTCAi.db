record(ai, "$(PV_NAME)")
{
    field(DESC, "$(DESC)")
    field(SCAN, "$(SCAN)")
    field(INP, "$(TC) CPP")
    field(HIGH, "45")
    field(HIHI, "70")
    field(LOW, "0")
    field(LOLO, "0")
    field(HHSV, "MAJOR")
    field(HSV, "MINOR")
    field(LSV, "MINOR")
    field(LLSV, "MAJOR")
    field(PREC, "1")
}
