record(ai, "$(PV_NAME)_A")
{
	field(SCAN, "I/O Intr")
	field(DTYP, "EtherIP")
	field(DESC, "$(DESC)")
	field(INP, "@$(PLC) $(TAG) S 0.5")
	field(EGU, "$(EGU)")
	field(HOPR, "$(HOPR)")
	field(LOPR, "$(LOPR)")
	field(HIHI, "$(HIHI)")
	field(HIGH, "$(HIGH)")
	field(LOW, "$(LOW)")
	field(LOLO, "$(LOLO)")
	field(HHSV, "MAJOR")
	field(HSV, "MINOR")
	field(LSV, "MINOR")
	field(LLSV, "MAJOR")
	field(PREC, "$(PREC)")
}
record(calc, "$(PV_NAME)_C")
{
	field(SCAN, "1 second")
	field(CALC, "A*B")
	field(INPA, "$(PV_NAME)_A.VAL")
	field(INPB, "$(INPB)")
}
