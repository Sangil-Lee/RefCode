record(mbbo, "$(P)_MODE_STATUS")
{
    field(DESC, "BEAM")
	field(ZRST, "N/A")
	field(ONST, "FILA")
	field(TWST, "ARC")
	field(THST, "HIPOT")
	field(FRST, "BEAM")
}
record(calc, "$(P)_FILA_MODE_C")
{
    field(DESC, "fila mode")
	field(FLNK, "$(P)_FILA_MODE NPP NMS")
	field(CALC, "A=1:1?0")
	field(INPA, "$(P)_MODE_STATUS.VAL CPP")
}
record(bi, "$(P)_FILA_MODE")
{
    field(DESC, "FILA mode")
	field(SCAN, "Passive")
	field(INP, "$(P)_FILA_MODE_C NPP NMS")
	field(OSV, "NO_ALARM")
	field(ZSV, "MAJOR")
}
