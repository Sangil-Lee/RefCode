  field(INPA, "$(SYS)_LTU_P0_SEC0_T1 CPP")
  field(OUT , "$(SYS)_STOP_T1_SEC1")
  field(FLNK, "$(SYS)_STOP_T1_SEC1")
}

record(mbbi, "$(SYS)_AMP_STAT") {
  field(DESC, "Status of Amplifier")
  field(SCAN, "I/O Intr")
  field(DTYP, "NI6122 MbbiRead")
  field(INP , "@amp_stat")
  field(ZRST, "Reset")
  field(ONST, "Ready")
  field(TWST, "Started")
  field(THST, "Error")
  field(ZRSV, "NO_ALARM")
  field(ONSV, "NO_ALARM")
  field(TWSV, "NO_ALARM")
  field(THSV, "MAJOR")
}

