# --------------------------------------------
# 2013.06.19 Start & Duration time
# --------------------------------------------

record(ao, "$(SYS)_START_TIME") {
  field(DESC, "Start time")
  field(DTYP, "Soft Channel")
}

record(ao, "$(SYS)_DURATION_TIME") {
  field(DESC, "Duration time of acquistion")
  field(DTYP, "Soft Channel")
}

record(calcout, "$(SYS)_LTU_T0_CALC") {
  field(DESC, "Calcuration for T0")
  field(SCAN, "Passive")
  field(CALC, "A+B")
  field(INPA, "$(SYS)_START_TIME CPP")
  field(INPB, "$(SYS)_CCS_BLIP_TIME CPP")
  field(OUT , "$(SYS)_LTU_P0_SEC0_T0 PP")
}

record(calcout, "$(SYS)_LTU_T1_CALC") {
  field(DESC, "Calcuration for T1")
  field(SCAN, "Passive")
  field(CALC, "A+B")
  field(INPA, "$(SYS)_LTU_P0_SEC0_T0 CPP")
  field(INPB, "$(SYS)_DURATION_TIME CPP")
  field(OUT , "$(SYS)_LTU_P0_SEC0_T1 PP")
}

record(calcout, "$(SYS)_START_TIME_CALC") {
  field(DESC, "Calcuration for Start time")
  field(SCAN, "Passive")
  field(CALC, "A-B")
  field(INPA, "$(SYS)_LTU_P0_SEC0_T0 CPP")
  field(INPB, "$(SYS)_CCS_BLIP_TIME CPP")
  field(OUT , "$(SYS)_START_TIME PP")
}

record(calcout, "$(SYS)_DURATION_CALC") {
  field(DESC, "Calcuration for Duration time")
  field(SCAN, "Passive")
  field(CALC, "B-A")
  field(INPA, "$(SYS)_LTU_P0_SEC0_T0 CPP")
  field(INPB, "$(SYS)_LTU_P0_SEC0_T1 CPP")
  field(OUT , "$(SYS)_DURATION_TIME PP")
}

