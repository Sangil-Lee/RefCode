record(calcout, "$(REC)_CALC") {
  field(CALC, "(A XOR B)?1:0")
  field(INPA, "$(PV_NAME)")
  field(INPB, "$(PV_NAME)_IN.VAL")
  field(FLNK, "$(PV_NAME)_OUT")
  field(SCAN, "Passive")
  field(OUT, "$(PV_NAME)_OUT PP")
}


record(bi, "$(PV_NAME)_IN") {
  field(DTYP, "EtherIP")
  field(INP, "@plc0 $(TAG_OUT) S 0.1")
  field(SCAN, ".5 second")
  field(ZNAM, "$(ZNAM)")
  field(ONAM, "$(ONAM)")
}


record(ao, "$(PV_NAME)_OUT") {
  field(DTYP, "EtherIP")
  field(OUT, "@plc0 $(TAG) S 0.1")
  field(OMSL, "supervisory")
}

record(bi, "$(PV_NAME)") {
  field(DESC, "$(DESC)")
  field(SCAN, "Passive")
  field(FLNK, "$(REC)_CALC")
  field(ZSV, "$(ZSV)")
  field(OSV, "$(OSV)")
  field(ZNAM, "$(ZNAM)")
  field(ONAM, "$(ONAM)")
  field(INP, "$(PV_NAME_REF) CPP")
}
